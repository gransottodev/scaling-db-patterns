# Node Environment
NODE_ENV=development

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
CACHE_TTL=300

# Database Master Configuration (Write Operations)
DB_MASTER_HOST=localhost
DB_MASTER_PORT=5432
DB_MASTER_USER=postgres
DB_MASTER_PASS=postgres_password

# Database Replica Configuration (Read Operations)
DB_SLAVE_HOST=localhost
DB_SLAVE_PORT=5433
DB_SLAVE_USER=postgres
DB_SLAVE_PASS=postgres_password

# Database Name
DB_NAME=scaling_db
